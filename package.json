{
  "name": "zehava-whatsapp-webhook",
  "version": "1.0.0",
  "description": "WhatsApp webhook processing system with EvolutionAPI, OpenAI, and Google Sheets",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration",
    "test:coverage": "jest --coverage",
    "test:deploy": "node test-deployment.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy"
  },
  "keywords": [
    "whatsapp",
    "webhook",
    "evolution-api",
    "openai",
    "langgraph",
    "google-sheets"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/langgraph": "^0.0.26",
    "@langchain/openai": "^0.0.14",
    "@prisma/client": "^5.7.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^128.0.0",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.5",
    "@types/supertest": "^2.0.16",
    "jest": "^29.7.0",
    "prisma": "^5.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
